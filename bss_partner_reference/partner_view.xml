<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>

		<record model="ir.ui.view" id="view_partner_bluestar_form">
			<field name="name">res.partner.bluestar.form.inherit</field>
			<field name="model">res.partner</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="base.view_partner_form" />
			<field name="arch" type="xml">
				<data>
					<field name="ref" readonly="1" />
				</data>
			</field>
		</record>

		<record id="view_bluestar_partner_reference_config_form" model="ir.ui.view">
			<field name="name">bluestar.partner.reference.config.form</field>
			<field name="model">bluestar.config</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="bss_config.view_bluestar_config_form" />
			<field name="arch" type="xml">
				<data>
					<separator string="Modules" position="after">
						<group string="Partner reference" colspan="4" col="4">
							<field name="generate_ref" />
						</group>
					</separator>
				</data>
			</field>
		</record>

	</data>
</openerp>
