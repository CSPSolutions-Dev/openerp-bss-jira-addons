<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>

		<record model="ir.ui.view" id="view_bluestar_bank_ch_form">
			<field name="name">bluestar.bank_ch.form</field>
			<field name="model">bluestar.bank_ch</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="">
					<field name="name" colspan="4" />
					<field name="group" />
					<field name="cb_number" />
					<field name="new_cb_number" />
					<field name="current_cb_number" select="1"/>
					<field name="branch_id" />
					<field name="sic_number" />
					<field name="headquarters" />
					<field name="cb_number_type" />
					<field name="valid_from" />
					<field name="sic_code" />
					<field name="eurosic_code" />
					<field name="language" />
					<field name="long_name" select="1"/>
					<field name="short_name" />
					<field name="home_address" />
					<field name="postal_address" />
					<field name="zip" select="1"/>
					<field name="location" select="1"/>
					<field name="phone" />
					<field name="fax" />
					<field name="telephone_code" />
					<field name="country_code" />
					<field name="postal_code" />
					<field name="swift" select="1"/>
				</form>
			</field>
		</record>

		<record model="ir.ui.view" id="view_bluestar_bank_ch_tree">
			<field name="name">bluestar.bank_ch.tree</field>
			<field name="model">bluestar.bank_ch</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="">
					<field name="group" />
					<field name="current_cb_number" />
					<field name="branch_id" />
					<field name="short_name" />
					<field name="long_name" />
					<field name="zip" />
					<field name="location" />
					<field name="swift" />
				</tree>
			</field>
		</record>

	</data>
</openerp>


